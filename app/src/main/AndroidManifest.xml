<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.mlkitbaseapis">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ml_kit_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TextRecognition" />
        <activity android:name=".FaceDetection" />
        <activity android:name=".BarcodeScanning" />
        <activity android:name=".ImageLabelling" />
        <activity android:name=".LandmarkRecognition"></activity>
    </application>

</manifest>